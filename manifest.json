{
    "manifest_version": 3,
    "name": "FSY youtube",
    "description": "Full Screen Youtube",
    "version": "0.1",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/watch*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}